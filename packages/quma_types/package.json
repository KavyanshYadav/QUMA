{
  "name": "@quma/types",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
    "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/quma_types",
        "main": "packages/quma_types/src/index.ts", 
        "tsConfig": "packages/quma_types/tsconfig.lib.json",

        "assets": ["packages/quma_types/*.md"]
      }
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@quma/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "tslib": "^2.3.0",
    "zod": "^4.1.11"
  }
}
