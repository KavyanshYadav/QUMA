  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      },
      "docker:backend": {
        "executor": "nx:run-commands",
        "options": {
          "command": [
            "docker build -t quma-backend -f packages/backend/Dockerfile ."
          ]
        }
      },
      "docker-compose-dev": {
        "executor": "nx:run-commands",
        "options": {
          "command": [
            "docker compose -f ./Docker/docker-compose-dev.yaml up --build"
          ]
        }
      }
    }
